<div class="container p-2 bg-light mt-5">
  <h2 class="text-center m-3">Enterprenuers List</h2>
  <table class="table bg-light p-2">
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Email</th>
        <th scope="col">Place</th>
        <th scope="col">Status</th>
        <th scope="col">action</th>
      </tr>
    </thead>
    <tbody>
      {{#each sellers}}
      <tr>
        <td scope="row">{{userName}}</td>
        <td>{{email}}</td>
        <td>{{place}}</td>
        <td>{{status}}</td>
        <td> <a href="/block-seller/{{id}}" class="btn btn-danger">Block</a></td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>