<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>
<body>
  <div class="container p-2 bg-light mt-5">
  <h2 class="text-center m-3">Staff Report</h2>
  <table class="table bg-light p-2">
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Email</th>
        <th scope="col">Mobile</th>
        <th scope="col">Status</th>
      </tr>
    </thead>
    <tbody>
      {{#each users}}
      <tr>
        <td scope="row">{{userName}}</td>
        <td>{{email}}</td>
        <td>{{mobile}}</td>
        <td>{{status}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
   <button onclick="printPage()" class="btn btn-primary mb-3">Print</button>
</div>
<script>
    function printPage() {
    window.print();
  }
</script>
</body>
</html>